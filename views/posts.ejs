<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Posts</title>
</head>

<body>
    <h1>Posts</h1>
    <ul>
        <% posts.forEach(function(post) { %>
            <li><strong>
                    <%= post.title %>
                </strong>: <%= post.content %>
            </li>
            <% }); %>
    </ul>
    <a href="/dashboard">Back to Dashboard</a>
</body>

</html>